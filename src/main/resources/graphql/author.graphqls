type Author {
    id: ID!
    firstName: String!
    lastName: String!
    bookList: [Book]!
}

type Query {
    findAllAuthors: [Author]!
    findAllAuthorsPage(page: Int!, size: Int!): [Author]!
    countAuthors: Int!
}

type Mutation {
    newAuthor(firstName: String!, lastName: String!) : Author!
}